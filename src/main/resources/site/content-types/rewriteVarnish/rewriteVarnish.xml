<?xml version="1.0" encoding="UTF-8"?>
<content-type>
    <display-name>Omskrivningsregler Varnish</display-name>
    <description>Lenker som skal g√• til XP</description>
    <super-type>base:structured</super-type>
    <form>
        <field-set>
            <items>
                <item-set name="requests">
                    <label>Lenker</label>
                    <occurrences minimum="0" maximum="0" />
                    <items>
                        <input name="requestUrl" type="TextLine">
                            <label>Lenke XP</label>
                            <occurrences minimum="1" maximum="1"/>
                        </input>
                        <input name="enableRule" type="CheckBox">
                            <label>Aktiver regel</label>
                            <config>
                                <alignment>right</alignment>
                            </config>
                        </input>
                    </items>
                </item-set>
            </items>
        </field-set>
    </form>
</content-type>
