<?xml version="1.0" encoding="UTF-8"?>
<content-type>
    <display-name>Tabell for statistikksider</display-name>
    <super-type>base:structured</super-type>
    <form>
        <input type="TextLine" name="title">
            <label>Tittel</label>
            <occurrences minimum="0" maximum="1" />
        </input>

        <input type="TextLine" name="tableId">
            <label>Lime inn ID</label>
            <occurrences minimum="1" maximum="1" />
        </input>

        <mixin name="footnote" />

        <mixin name="sources" />

        <input type="TextArea" name="correctionNotice">
            <label>Rettemelding</label>
            <occurrences minimum="0" maximum="1" />
        </input>
    </form>
</content-type>
