<part>
  <display-name>Highchart</display-name>
  <form>
    <input name="highchart" type="ContentSelector">
      <label>Graf</label>
      <occurrences minimum="1" maximum="0"/>
      <config>
        <allowPath>${site}</allowPath>
        <allowType>highchart</allowType>
        <allowType>combinedGraph</allowType>
      </config>
    </input>
  </form>
</part>
