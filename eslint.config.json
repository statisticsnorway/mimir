{
    "extends": ["google"],
    "rules": {
        "comma-dangle": ["error", {
            "arrays": "never",
            "objects": "never",
            "imports": "never",
            "exports": "never",
            "functions": "ignore"
        }],
        "no-extend-native": 0,
        "quotes": 1,
        "max-len": [2, 250],
        "linebreak-style": 0,
        "brace-style": [1, "stroustrup"],
        "camelcase": 1,
        "semi": 1,
        "no-var": 1,
        "require-jsdoc": 0,
        "block-spacing": 1,
        "space-before-function-paren": 1,
        "eol-last": 1,
        "no-multi-spaces": 1,
        "spaced-comment": 1,
        "func-call-spacing": 1,
        "new-cap": 1,
        "keyword-spacing": 1,
        "key-spacing": 1,
        "quote-props": 1,
        "comma-spacing": 1,
        "no-unused-vars": 1,
        "no-invalid-this": 1,
        "indent": 0,
        "semi": 0,
        "object-curly-spacing": 0,
        "no-irregular-whitespace": 0
    },
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    }
}
